public class Triangle {
    private double side1;
    private double side2;
    private double side3;

    public Triangle(double side1, double side2, double side3) {
        this.side1 = side1;
        this.side2 = side2;
        this.side3 = side3;
    }

    public boolean isRight() {
        // Check if the triangle satisfies the Pythagorean theorem
        return (side1 * side1 + side2 * side2 == side3 * side3) ||
               (side1 * side1 + side3 * side3 == side2 * side2) ||
               (side2 * side2 + side3 * side3 == side1 * side1);
    }

    public boolean isScalene() {
        return !isIsosceles();
    }

    public boolean isIsosceles() {
        return side1 == side2 || side1 == side3 || side2 == side3;
    }

    public boolean isEquilateral() {
        return side1 == side2 && side1 == side3;
    }

    public static void main(String[] args) {
        Triangle triangle1 = new Triangle(3, 4, 5);
        Triangle triangle2 = new Triangle(5, 5, 7);
        Triangle triangle3 = new Triangle(6, 6, 6);

        System.out.println("Triangle 1 - Right: " + triangle1.isRight());
        System.out.println("Triangle 1 - Scalene: " + triangle1.isScalene());
        System.out.println("Triangle 1 - Isosceles: " + triangle1.isIsosceles());
        System.out.println("Triangle 1 - Equilateral: " + triangle1.isEquilateral());

        System.out.println("Triangle 2 - Right: " + triangle2.isRight());
        System.out.println("Triangle 2 - Scalene: " + triangle2.isScalene());
        System.out.println("Triangle 2 - Isosceles: " + triangle2.isIsosceles());
        System.out.println("Triangle 2 - Equilateral: " + triangle2.isEquilateral());

        System.out.println("Triangle 3 - Right: " + triangle3.isRight());
        System.out.println("Triangle 3 - Scalene: " + triangle3.isScalene());
        System.out.println("Triangle 3 - Isosceles: " + triangle3.isIsosceles());
        System.out.println("Triangle 3 - Equilateral: " + triangle3.isEquilateral());
    }
}
